<% stat = problem_list_box %>
<% ptype = stat.problem_type %>
<% $stderr.puts stat.inspect %>
<% $stderr.puts " => #{stat.color_status}" %>
<% col = stat.color_status %>

<div class="problem_list_box <%= col %>-bord">
  <p><%= ptype.to_s %></p>
  <%= link_to "Let me try", problem_set_do_path(:name => @problem_set.id, :pid => ptype.id), remote:true %>
</div>
