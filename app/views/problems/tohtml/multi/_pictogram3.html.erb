<table id="pictogramtable">
    <% number.times do |row| %>
      <tr>
        <td>
        <p>
         <%=arr[row]%>
        </p>
        </td>
        <td><canvas id="myCanvas_<%="#{row}"%>" width=<%= canvaswidth%> height=<%= canvasheight%> style="border:1px solid #d3d3d3;">
Your browser does not support the HTML5 canvas tag.</canvas>
        </td>

        <td>
         <img border="border:1px solid #d3d3d3;" src="/assets/1.jpg" onclick="drawimage3(<%="#{row}"%>,<%= startx%>,<%= starty%>,<%= width%>,<%= height%>,<%= distance%>)" width="<%= canvasheight%>" height="<%= canvasheight%>">
        </td>
        <td>
         <img border="border:1px solid #d3d3d3;" src="/assets/2.jpg" onclick="drawimage4(<%="#{row}"%>,<%= startx%>,<%= starty%>,<%= width%>,<%= height%>,<%= distance%>)" style="width:<%= canvasheight%>/2px;height:<%= canvasheight%>px;">
        </td>
      </tr>
    <%end %>
</table>
<table id=pictogram_table style="display:none">
    <% number.times do |row| %>
	<tr>
        <td><%= hidden_field_tag("#{pictogram3.name}_#{row}", '0')%></td>
    </tr>
    <%end %>
</table>
