    <%height= pictogram3.height.to_i%>
    <%distance= pictogram3.distance.to_i%>
	<%canvaswidth = pictogram3.canvaswidth.to_i%>
    <%canvasheight= pictogram3.canvasheight.to_i%>
    <%numtimes = pictogram3.numtimes.to_i%>
    <%check = pictogram3.check.to_i%>
