 <div class=row-fluid>
    <%= render 'students/student_info', :@student => current_user %>
    <h3 style="display:inline-block;">Learning</h3>
    <ul class="breadcrumb">
        <li><b>6th Grade </b><span class="divider">&amp;</span></li>
        <li><%= link_to "Overview", studenthome_path %><span class="divider">&amp;</span></li>
        <li><%= link_to @pset.name, pset_path(:name => @pset.id) %><span class="divider">&amp;</span></li>
        <li class=active><%= "Quiz Result" %></li>
    </ul>
</div>
 <ul class=column-list>
  	<h2>Quiz Result</h2>
    <% @answers.each do |answer| %>
      	<li>
      		<%= link_to answer.problem_type.to_s, "#" %> : <%= answer.correct.to_s %>
      		<% if m_unpack(answer.response).first.last.blank? %>
      			<%= link_to "Please give an answer", left_quiz_problem_path(@instance, :problem_id => answer.problem_id), :class => "pull-right" %>
      		<% end %>
      	</li>
    <% end %>
</ul>

