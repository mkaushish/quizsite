<div class=results style="position:relative">
<div class="np_head">
    <h1>Calculations</h1>
    <div class="tbicons plug" id='np_mult'> 
      <span class="glyphicon glyphicon-remove" ></span>
    </div>
    <div class="tbicons plug" id='np_sub'> 
      <span class="glyphicon glyphicon-minus" ></span>
    </div>
    <div class="tbicons plug" id='np_add'>
      <span class="glyphicon glyphicon-plus" ></span>
    </div>
  </div>
  <div id='calc' class='calculator'>
    <%= render 'notepad/calc_form' %>
  </div>
  <div id='ops_form'></div>
  
  <br></br>
  <%= link_to_function "Save", "", :class => 'center_btn button-blue', :id => "sv_opform" %>
</div>
