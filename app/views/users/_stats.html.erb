<div id="quizzes"> 
  <ul> 
    <li class="selected">Fractions</li>
    <li class="unselected">Chapter 2</li>
    <li class="unselected">Chapter 1</li>
  </ul>
</div>
<div id="problemscores">
  <div id=smartscore style="background-color:#4545cf;">
    Your SmartScore for this quiz is 86
  </div>
  <ul>
    <% ptypes = [
      ["Convert to Improper Fractions", "84"],
      ["Convert to Mixed Fractions", "90"],
      ["Reduce Fractions", "57"],
      ["Fill Fractions",   "76"],
      ["Compare Like Fractions", "89"],
      ["Compare Unlike Fractions", "96"]] %>
    <% ptypes.each do |ptype| %>
      <li>
      <%= ptype.first %>
      <div class="smartscore <%=smartscore_class(ptype.last)%>"><%= ptype.last %></div>
      </li>
    <% end %>
  </ul>
</div>
