<div class=nav-collapse>
  <%= form_for(:session, {:url => sessions_path, :html => {:class => "mynavbar-form"}, :remote => true}) do |f| %>
    <%= hidden_field_tag 'email', "exampleuser@gmail.com" %>
    <%= hidden_field_tag 'password', "examplepassword" %>
    <ul class="nav pull-right">
      <li class=divider-vertical></li>
      <li class=text id=userfield> <%= f.text_field :notemail, :placeholder=>"I'm Not Real" %> </li>
      <li class=text id=psswfield> <%= f.password_field :notpassword, :placeholder => "Click Sign In" %> </li>
      <li class=button ><%= f.submit "Sign in",  :class=>"btn-success" %>
      <%= f.submit "Register", :class=>"btn-primary", :disabled=>"true" %></li>
    </ul>
  <% end %>
</div>
