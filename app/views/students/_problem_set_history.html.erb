<div class="tab-pane" id="chap_problem_set_<%= @problem_set.id %>">
	<div class="column-listheader">
		Problem Types
	</div>
	<div class="column-listdiv">
		<ul class='column-list2 column-list22' id="column_list">	  							
			<%- @problem_set_history.each do |problem_set_history| -%>
				<li><%= link_to problem_set_history[1], get_problem_types_history_path(@student.id, problem_type_id: problem_set_history[0]), remote: :true %> 
				<span style="float:right;"><%= ( problem_set_history[2].to_s + "/" + problem_set_history[3].to_s) %></span></li>
			<% end %>
		</ul>		
	</div>     
</div>