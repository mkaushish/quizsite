<!DOCTYPE HTML>
<html>
<head>
  <title><%= title %></title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <% #javascript_include_tag :defaults %>
  <%= csrf_meta_tags %>
  <%= yield :head %>
</head>
<body <%= jsonload %>>
  <%= render 'shared/nav' %>
  <div class="container" <%= container_height_style %>>
    <%= render 'shared/errors' %>
    <%= yield %>
    <p style="clear:both;">
  </div>

  <%# debug(params) if Rails.env.development? %>
</body>
</html>
